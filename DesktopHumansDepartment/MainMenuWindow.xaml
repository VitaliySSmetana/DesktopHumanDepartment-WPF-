<Window x:Class="DesktopHumansDepartment.MainMenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopHumansDepartment"
        mc:Ignorable="d"
        Title="MainMenuWindow" Height="300" Width="300">
    <Grid>
        <Button x:Name="departments" Content="Departments" HorizontalAlignment="Left" Margin="110,26,0,0" VerticalAlignment="Top" Width="75" Click="departments_Click"/>
        <Button x:Name="positions" Content="Positions" HorizontalAlignment="Left" Margin="110,62,0,0" VerticalAlignment="Top" Width="75" Click="positions_Click"/>
        <Button x:Name="employees" Content="Employees" HorizontalAlignment="Left" Margin="110,100,0,0" VerticalAlignment="Top" Width="75" Click="employees_Click"/>
        <Button x:Name="projects" Content="Projects" HorizontalAlignment="Left" Margin="110,137,0,0" VerticalAlignment="Top" Width="75" Click="projects_Click"/>
        <Button x:Name="search" Content="Search" HorizontalAlignment="Left" Margin="110,175,0,0" VerticalAlignment="Top" Width="75" Click="search_Click"/>
        <Button x:Name="exit" Content="Exit" HorizontalAlignment="Left" Margin="110,215,0,0" VerticalAlignment="Top" Width="75" Click="exit_Click"/>

    </Grid>
</Window>
