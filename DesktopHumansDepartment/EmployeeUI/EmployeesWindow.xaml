<Window x:Class="DesktopHumansDepartment.EmployeesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopHumansDepartment"
        mc:Ignorable="d"
        Title="EmployeesWindow" Height="300" Width="300">
    <Grid>
        <Button x:Name="addEmployee" Content="Add Employee" HorizontalAlignment="Left" Margin="100,31,0,0" VerticalAlignment="Top" Width="104" Click="addEmployee_Click"/>
        <Button x:Name="back" Content="Back" HorizontalAlignment="Left" Margin="100,163,0,0" VerticalAlignment="Top" Width="104" Click="back_Click"/>
        <Button x:Name="exit" Content="Exit" HorizontalAlignment="Left" Margin="100,203,0,0" VerticalAlignment="Top" Width="104" Click="exit_Click"/>

    </Grid>
</Window>
